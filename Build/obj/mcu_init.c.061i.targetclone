EcuM_Init ()
{
  <bb 2> :
  MCU_clocks ();
  OsIf_Init (0B);
  Gpt_conf ();
  Port_Init (&Port_Config);
  Platform_Init (0B);
  return;

}


Gpt_conf ()
{
  <bb 2> :
  Gpt_Init (&Gpt_Config);
  Gpt_EnableNotification (0);
  return;

}


MCU_clocks ()
{
  <bb 2> :
  Mcu_Init (&Mcu_Config);
  Mcu_SetMode (0);
  Mcu_InitClock (0);
  return;

}


